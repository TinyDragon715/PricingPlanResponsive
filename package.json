{
    "name": "remote-learner-frontend",
    "version": "2.0.0",
    "scripts": {
        "start": "ng serve --progress",
        "build": "ng build",
        "build-registration-staging": "ng build",
        "build-registration-prod": "ng build --prod --configuration registration.prod",
        "build-registration-qa": "ng build --prod --configuration registration.qa",
        "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
        "gitbuild": "ng build --prod --base-href /start-javascript/sb-admin-material/master/dist/",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "deploy": "aws s3 cp ./dist/ s3://accounts-stellar-online/ --recursive",
        "invalidate": "aws cloudfront create-invalidation --distribution-id EDE7B72GHGQ27 --paths '/*'",
        "allin": "npm run build-registration-prod && npm run deploy && npm run invalidate",
        "allin-staging": "npm run build-registration-staging && rsync -avz ./dist/* ili-dev-secret-sauce:~/stellar-accounts-staging",
        "allin-qa": "npm run build-registration-qa && rsync -avz ./dist/* ili-dev-secret-sauce:~/stellar-accounts-qa"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "9.1.1",
        "@angular/cdk": "9.2.0",
        "@angular/common": "9.1.1",
        "@angular/compiler": "9.1.1",
        "@angular/core": "9.1.1",
        "@angular/flex-layout": "9.0.0-beta.29",
        "@angular/forms": "9.1.1",
        "@angular/http": "7.2.16",
        "@angular/material": "9.2.0",
        "@angular/platform-browser": "9.1.1",
        "@angular/platform-browser-dynamic": "9.1.1",
        "@angular/router": "9.1.1",
        "@ng-idle/core": "^11.1.0",
        "@ng-idle/keepalive": "^11.0.3",
        "@ng-select/ng-select": "^4.0.4",
        "@ngx-translate/core": "12.1.2",
        "@ngx-translate/http-loader": "4.0.0",
        "@types/lodash": "^4.14.158",
        "@types/stripe-v3": "^3.1.22",
        "@vimeo/player": "^2.12.2",
        "apexcharts": "3.20.2",
        "chart.js": "2.9.3",
        "chartjs-plugin-datalabels": "^0.7.0",
        "compass-mixins": "^0.12.12",
        "core-js": "3.6.5",
        "credit-card-logos": "^1.0.5",
        "faker": "^5.1.0",
        "file-saver": "^2.0.2",
        "flag-icon-css": "^4.1.7",
        "image-map": "^2.0.1",
        "less-loader": "^7.2.1",
        "moment": "^2.29.1",
        "ng-apexcharts": "1.5.1",
        "ng2-charts": "^2.3.0",
        "ngx-ckeditor": "^8.0.0",
        "ngx-device-detector": "^1.4.6",
        "ngx-image-cropper": "^3.3.3",
        "object-path": "^0.11.4",
        "rxjs": "6.5.5",
        "web-animations-js": "^2.3.2",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.1",
        "@angular/cli": "9.1.1",
        "@angular/compiler-cli": "9.1.1",
        "@angular/language-service": "9.1.1",
        "@compodoc/compodoc": "^1.1.11",
        "@types/googlemaps": "^3.39.13",
        "@types/jasmine": "3.5.10",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "^13.11.1",
        "codelyzer": "^5.2.2",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "5.0.1",
        "karma": "^5.0.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.1",
        "karma-jasmine": "3.1.1",
        "karma-jasmine-html-reporter": "1.5.3",
        "node-pre-gyp": "^0.14.0",
        "protractor": "5.4.3",
        "ts-node": "8.8.2",
        "tslib": "1.11.1",
        "tslint": "6.1.1",
        "typescript": "3.8.3"
    }
}
